{
    "name": "ng-docx",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "server": "node server.js",
        "start": "ng serve ng-docx-sandbox",
        "build": "ng build ng-docx",
        "build:lib": "ng build ng-docx && npm run copy-lib-assets && node copy_some_docs.js",
        "copy-lib-assets": "cpx 'projects/ng-docx/src/lib/assets/**/' 'dist/ng-docx/assets'",
        "build:lib-watch": "ng build ng-docx --watch",
        "deploy": "ng build ng-docx-sandbox --aot --prod",
        "deploy-ghpages": "ng deploy --base-href='/ng-docx/' --repo=https://github.com/CarlosTenorio/ng-docx.git --name='CarlosTenorio' --email=cartenorioperez@gmail.com",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ng build ng-docx-sandbox"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "~11.2.14",
        "@angular/cdk": "^11.2.13",
        "@angular/cli": "~11.2.14",
        "@angular/common": "~11.2.14",
        "@angular/compiler": "~11.2.14",
        "@angular/compiler-cli": "~11.2.14",
        "@angular/core": "~11.2.14",
        "@angular/forms": "~11.2.14",
        "@angular/material": "^11.2.13",
        "@angular/platform-browser": "~11.2.14",
        "@angular/platform-browser-dynamic": "~11.2.14",
        "@angular/router": "~11.2.14",
        "express": "^4.17.1",
        "mark.js": "^8.11.1",
        "ng-docx": "2.0.0",
        "ngx-markdown": "^10.1.1",
        "rxjs": "~6.5.4",
        "tslib": "^2.0.0",
        "typescript": "~4.0.8",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1201.3",
        "@angular-devkit/build-angular": "~0.1102.14",
        "@angular/language-service": "~11.2.14",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "angular-cli-ghpages": "^1.0.0-rc.2",
        "codelyzer": "^6.0.0",
        "copy": "^0.3.2",
        "cpx": "1.5.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.3.4",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^11.2.4",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0"
    },
    "engines": {
        "node": "12.x",
        "npm": "6.x"
    },
    "keywords": [
        "angular",
        "docs",
        "documentation",
        "markdown",
        "ng"
    ]
}
