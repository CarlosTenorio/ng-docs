{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ng-docs": {
            "projectType": "library",
            "root": "projects/ng-docs",
            "sourceRoot": "projects/ng-docs/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-ng-packagr:build",
                    "options": {
                        "tsConfig": "projects/ng-docs/tsconfig.lib.json",
                        "project": "projects/ng-docs/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/ng-docs/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/ng-docs/src/test.ts",
                        "tsConfig": "projects/ng-docs/tsconfig.spec.json",
                        "karmaConfig": "projects/ng-docs/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["projects/ng-docs/tsconfig.lib.json", "projects/ng-docs/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "ng-docs-sandbox": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/ng-docs-sandbox",
            "sourceRoot": "projects/ng-docs-sandbox/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ng-docs-sandbox",
                        "index": "projects/ng-docs-sandbox/src/index.html",
                        "main": "projects/ng-docs-sandbox/src/main.ts",
                        "polyfills": "projects/ng-docs-sandbox/src/polyfills.ts",
                        "tsConfig": "projects/ng-docs-sandbox/tsconfig.app.json",
                        "aot": true,
                        "assets": ["projects/ng-docs-sandbox/src/favicon.ico", "projects/ng-docs-sandbox/src/assets"],
                        "styles": [
                            "projects/ng-docs-sandbox/src/styles.scss",
                            "node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
                            "node_modules/ng-docs/src/lib/assets/styles/ng-docs.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/ng-docs-sandbox/src/environments/environment.ts",
                                    "with": "projects/ng-docs-sandbox/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "ng-docs-sandbox:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "ng-docs-sandbox:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ng-docs-sandbox:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/ng-docs-sandbox/src/test.ts",
                        "polyfills": "projects/ng-docs-sandbox/src/polyfills.ts",
                        "tsConfig": "projects/ng-docs-sandbox/tsconfig.spec.json",
                        "karmaConfig": "projects/ng-docs-sandbox/karma.conf.js",
                        "assets": ["projects/ng-docs-sandbox/src/favicon.ico", "projects/ng-docs-sandbox/src/assets"],
                        "styles": ["projects/ng-docs-sandbox/src/styles.scss"],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/ng-docs-sandbox/tsconfig.app.json",
                            "projects/ng-docs-sandbox/tsconfig.spec.json",
                            "projects/ng-docs-sandbox/e2e/tsconfig.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/ng-docs-sandbox/e2e/protractor.conf.js",
                        "devServerTarget": "ng-docs-sandbox:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "ng-docs-sandbox:serve:production"
                        }
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    },
    "defaultProject": "ng-docs"
}
